{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Ranch-Agent (Extension Folder)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "extension: build"
    }
  ]
}
