{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Ranch-Agent",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/extension"
      ],
      "outFiles": [
        "${workspaceFolder}/extension/dist/**/*.js"
      ],
      "preLaunchTask": "extension: build"
    }
  ]
}
